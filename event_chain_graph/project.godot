[application]

config/name="event_chain_graph"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[autoload]

LineConstant="*res://line/line_constant.gd"
LineMgr="*res://line/line_mgr.gd"
BranchMgr="*res://branch/branch_mgr.gd"
BranchConstant="*res://branch/branch_constant.gd"
EditConstant="*res://edit_constant.gd"

[editor_plugins]

enabled=PoolStringArray( "event_chain" )

[event_chain_graph]

generation/enable_multithreading=true
generation/delay_before_generation=75
user_interface/inline_vector_fields=false
user_interface/group_nodes_by_type=false

[rendering]

environment/default_environment="res://default_env.tres"
